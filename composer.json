{
    "name": "dasprid/container-interop-doctrine",
    "type": "library",
    "description": "Doctrine factories for container-interop",
    "homepage": "https://github.com/DASPRiD/container-interop-doctrine",
    "license" : "BSD-2-Clause",
    "require": {
        "php": "5.5.*|5.6.*|7.0.*|7.1.*",
        "doctrine/orm": "^2.5",
        "container-interop/container-interop": "^1.1",
        "doctrine/dbal": "^2.5",
        "doctrine/common": "^2.6|^2.7"
    },
    "autoload": {
        "psr-4": {
            "ContainerInteropDoctrine\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "^2.5"
    },
    "autoload-dev": {
        "psr-4": {
            "ContainerInteropDoctrineTest\\": "test/"
        }
    }
}
